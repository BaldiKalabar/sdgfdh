        a.write('1-–ó–ê–†–ò–ö–†–û–õ–ï–ù(–î–æ—Å—Ç–∏–∂–µ–Ω–∏–µ)')
        a.close()
        exit()
    if p1=='–Ω–µ—Ç':
        print('–Ω—É –æ–∫–µ–π')
    time.sleep(1)
    p1=input('???-–∫—Å—Ç–∞—Ç–∏ —Ç—ã –≤–µ–¥—å –∑–Ω–∞–µ—à –ø—Ä–æ –ú–Ø–£–ö–†–û–õ–ê?-+(–¢—ã –æ –∫–æ–º)...(–ù–µ—Ç)').lower()
    while p1 !='—Ç—ã –æ –∫–æ–º':
        p1=input('???-–∫—Å—Ç–∞—Ç–∏ —Ç—ã –≤–µ–¥—å –∑–Ω–∞–µ—à –ø—Ä–æ –ú–Ø–£–ö–†–û–õ–ê?(–¢—ã –æ –∫–æ–º)').lower()
    print('???-–Ω—É –∫–æ—Ä–æ—á–µ —Å–ª—ã—à–∏—à—å –∑–≤—É–∫ –º—è—É–∫–∞–Ω—å—è –ø—Ä—è—á—Å—è')
    webbrowser.open('https://www.youtube.com/watch?v=jTEN0bngTFM', new=2)
    p1=input('???-–û–∫–µ–µ–µ–π —è —Å–µ–π—á–∞—Å(–•–æ—Ä–æ—à–æ).....(–ü—Ä—è—á–µ–º—Å—è)').lower()
    while p1 !='—Ö–æ—Ä–æ—à–æ' and p1 !='–ø—Ä—è—á–µ–º—Å—è':
        p1=input('(–•–æ—Ä–æ—à–æ).....(–ü—Ä—è—á–µ–º—Å—è)').lower()
        while p1 !='—Ö–æ—Ä–æ—à–æ' and p1 !='–ø—Ä—è—á–µ–º—Å—è':
            webbrowser.open('https://www.youtube.com/watch?v=hvL1339luv0', new=2)
    if p1=='—Ö–æ—Ä–æ—à–æ':
        webbrowser.open('https://www.youtube.com/watch?v=hvL1339luv0', new=2)
        time.sleep(1)
        print('–ú–Ø–£ –ú–ú–Ø–£')
        a.write('2-–ó–ê–ú–Ø–£–ö–ê–Ω(–î–æ—Å—Ç–∏–∂–µ–Ω–∏–µ)')
        a.close()
        exit(0)
    if p1=='–ü—Ä—è—á–µ–º—Å—è':
        print('–ú–Ø–£ –ú–Ø–£–£–£–£–£–£–£–£–£–£–£–£–£–£–£–£–£–£–£–£–£–£')
        a.write('–ú—è—É –ú—è—É –ú—è—É(–î–æ—Å—Ç–∏–∂–µ–Ω–∏–µ)')
        a.close()
    print('–≤—ã—Ö–æ–¥–∏–º –∏–∑ —à–∫–∞—Ñ–∞_+_')
